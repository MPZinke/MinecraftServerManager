	<body>

		<form
			method="POST"
		>
			<table
			>
				<tbody>
					<tr>
						<td>
							Name
						</td>
						<td>
							<input
								name="name"
							/>
						</td>
					</tr>
					<tr>
						<td>
							<a
								href="/versions"
							>
								Version
							</a>
						</td>
						<td>
							<select
								name="version_id"
							>
								{% for version in versions %}
									<option value="{{ version["id"] }}">
										{{ version["tag"] }}
									</option>
								{% endfor %}
							</select>
						</td>
					</tr>
					<tr>
						<td>
							Notes
						</td>
						<td>
							<input
								name="notes"
							/>
						</td>
					</tr>
				</tbody>
			</table>
			<button
				type="submit"
			>
				Add
			</button>
		</form>
	</body>
</html>
