<html>


	<header>
		<a
			href="/"
		>
			âŒ‚
		</a>
		&gt;
		<a
			href="/worlds"
		>
			Worlds
		</a>
		&gt; New
	</header>

	<body>

		<form
			method="POST"
			enctype="multipart/form-data"
		>
			<table
			>
				<tbody>
					<tr>
						<td>
							Name
						</td>
						<td>
							<input
								name="name-input"
							/>
						</td>
					</tr>
					<tr>
						<td>
							<a
								href="/versions"
							>
								Version
							</a>
						</td>
						<td>
							<select
								name="version_id-select"
							>
								{% for version in versions %}
									<option
										value="{{ version.id }}"
										{% if version.id == version_id %}
											selected
										{% endif %}
									>
										{{ version.tag }}
									</option>
								{% endfor %}
							</select>
						</td>
					</tr>
					<tr>
						<td>
							(Optional) World File
						</td>
						<td>
							<input
								name="file-input"
								type="file"
							/>
						</td>
					</tr>
					<tr>
						<td>
							Notes
						</td>
						<td>
							<input
								name="notes-input"
							/>
						</td>
					</tr>
				</tbody>
			</table>
			<button
				type="submit"
			>
				Add
			</button>
		</form>
	</body>
</html>
